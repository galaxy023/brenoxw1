const url = window.location.href; // Obtém a URL da página atual
let server = '';

// Verifica se a URL contém informações sobre o servidor
if (url.includes('meuservidor.com')) {
    server = 'Meu Servidor';
} else if (url.includes('outroservidor.com')) {
    server = 'Outro Servidor';
} else {
    server = 'Servidor Desconhecido';
}

console.log(`O servidor responsável pela ação é: ${server}`);
